{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "cookie-banner": [
        "dist/cookie-banner/cookie-banner",
        "dist/cookie-banner"
      ],
      "multi-select": [
        "dist/multi-select/multi-select",
        "dist/multi-select"
      ]
    }
  }
}